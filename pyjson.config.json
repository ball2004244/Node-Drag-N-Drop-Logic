{
  "KEYWORDS": {
    "print": "print(%s)",
    "comment": "# %s",
    "return": "return %s"
  },
  "START_KEYWORDS": {
    "for": "for %s:",
    "while": "while %s:",
    "if": "if %s:",
    "elif": "elif %s:",
    "else": "else: %s",
    "func": "def %s:",
    "class": "class %s:"
  },
  "END_KEYWORDS": {
    "endfor": "%s",
    "endwhile": "%s",
    "endif": "%s",
    "endelif": "%s",
    "endelse": "%s",
    "endfunc": "%s",
    "endclass": "%s"
  },
  "VAR_KEYWORDS": {
    "int": "%s = int(%s)",
    "str": "%s = str(%s)",
    "float": "%s = float(%s)",
    "bool": "%s = bool(%s)"
  },
  "BLOCK_KEYWORDS": {
    "for": "for %s:\n",
    "while": "while %s:\n",
    "if": "if %s:\n",
    "elif": "elif %s:\n",
    "else": "else: %s\n",
    "function": "def %s:\n",
    "class": "class %s:\n"
  }
}
