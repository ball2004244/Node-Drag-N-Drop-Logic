{
  "KEYWORDS": {
    "print": "print(%s)",
    "comment": "# %s",
    "return": "return %s"
  },
  "START_KEYWORDS": {
    "for": "for %s:",
    "while": "while %s:",
    "if": "if %s:",
    "else": "else: %s",
    "function": "def %s:",
    "class": "class %s:"
  },
  "END_KEYWORDS": {
    "endfor": "%s",
    "endwhile": "%s",
    "endif": "%s",
    "endelse": "%s",
    "endfunction": "%s",
    "endclass": "%s"
  },
  "VAR_KEYWORDS": {
    "int": "%s = int(%s)",
    "str": "%s = str(%s)",
    "float": "%s = float(%s)",
    "bool": "%s = bool(%s)"
  },
  "ADV_DATA_TYPES": {
    "list": "%s = list(%s)",
    "tuple": "%s = tuple(%s)",
    "set": "%s = set(%s)",
    "dict": "%s = json.loads(%s)"
  },
  "BLOCK_KEYWORDS": {
    "block": "\n"
  }
}
