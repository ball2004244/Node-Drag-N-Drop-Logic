{
  "KEYWORDS": {
    "print": "print(%s)",
    "comment": "# %s",
    "return": "return %s"
  },
  "START_KEYWORDS": {
    "for": "for %s:",
    "while": "while %s:",
    "if": "if %s:",
    "elif": "elif %s:",
    "else": "else: %s",
    "function": "def %s:",
    "class": "class %s:"
  },
  "END_KEYWORDS": {
    "endfor": "%s",
    "endwhile": "%s",
    "endif": "%s",
    "endelif": "%s",
    "endelse": "%s",
    "endfunction": "%s",
    "endclass": "%s"
  },
  "VAR_KEYWORDS": {
    "int": "%s = int(%s)",
    "str": "%s = str(%s)",
    "float": "%s = float(%s)",
    "bool": "%s = bool(%s)"
  },
  "ADV_DATA_TYPES": {
    "list": "%s = list(%s)",
    "tuple": "%s = tuple(%s)",
    "set": "%s = set(%s)",
    "dict": "%s = dict(%s)"
  },
  "BRACKET_KEYWORDS": {
    "for": "for %s:\n",
    "while": "while %s:\n",
    "if": "if %s:\n",
    "elif": "elif %s:\n",
    "else": "else: %s\n",
    "function": "def %s:\n",
    "class": "class %s:\n"
  },
  "BLOCK_KEYWORDS": {
    "block": "\n"
  },
  "NON_PARAM_KEYWORDS": {
    "break": "break",
    "continue": "continue",
    "pass": "pass"
  }
}
